{
  "name": "getphred/pairity",
  "description": "Partitioned-model PHP ORM (DTO/DAO), Query Builder, Raw SQL, multi-DB via PDO.",
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Pairity Contributors" }
  ],
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Pairity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pairity\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
  ,
  "bin": [
    "bin/pairity"
  ]
}
